local service = 3432
local secret = "55acd596-9e84-4dbd-934e-9def6297d4d3"
local useNonce = true
local onMessage = function(message) game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", { Text = message }) end
local mainScript = "https://raw.githubusercontent.com/ZusumeHub/ZusumeHub/refs/heads/main/opws"

repeat task.wait() until game:IsLoaded() and game.Players.LocalPlayer

-- Sistem Key Manager
local function saveKey(key)
    if writefile then
        writefile("platoboost_key.txt", key)
    else
        (syn and syn.queue_on_teleport or queue_on_teleport)('_G.savedKey = "'..key..'"')
    end
end

local function loadSavedKey()
    local key = ""
    if readfile and pcall(readfile, "platoboost_key.txt") then
        key = readfile("platoboost_key.txt")
    end
    return _G.savedKey or key
end

-- Sistem Verifikasi
local fRequest = request or (syn and syn.request) or http_request
local HttpService = game:GetService("HttpService")
local cachedLink, cachedTime = "", 0

local function lDigest(input)
    local hash = {}
    for i = 1, #input do
        hash[i] = string.byte(input, i)
    end
    return table.concat(hash, ""):gsub(".", function(c) return string.format("%02x", c:byte()) end)
end

local function verifyKey(key)
    local response = fRequest({
        Url = "https://api.platoboost.com/public/whitelist/"..service.."?identifier="..lDigest(tostring(game:GetService("Players").LocalPlayer.UserId)).."&key="..key,
        Method = "GET"
    })
    
    if response.StatusCode == 200 then
        local data = HttpService:JSONDecode(response.Body)
        if data.success and data.data.valid then
            saveKey(key)
            return true
        end
    end
    return false
end

-- Auto-Verify Key yang Tersimpan
local savedKey = loadSavedKey()
if savedKey and verifyKey(savedKey) then
    loadstring(game:HttpGet(mainScript))()
    return
end

-- Modern GUI Design
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local gui = Instance.new("ScreenGui")
if syn and syn.protect_gui then
    syn.protect_gui(gui)
    gui.Parent = game:GetService("CoreGui")
else
    gui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
end

