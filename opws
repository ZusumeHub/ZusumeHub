AutoAttackButton.Size = UDim2.new(0.8, 0, 0, 40)
AutoAttackButton.Position = UDim2.new(0.1, 0, 0, 375)
AutoAttackButton.BackgroundColor3 = Color3.fromRGB(200, 70, 70)
AutoAttackButton.BorderSizePixel = 0
AutoAttackButton.Font = Enum.Font.GothamBold
AutoAttackButton.TextColor3 = Color3.fromRGB(255, 255, 255)
AutoAttackButton.Text = "üó°Ô∏è Auto Attack: OFF"
AutoAttackButton.TextSize = 14
AutoAttackButton.Parent = HackScroll

local AutoAttackCorner = Instance.new("UICorner")
AutoAttackCorner.CornerRadius = UDim.new(0, 8)
AutoAttackCorner.Parent = AutoAttackButton

AutoAttackButton.MouseButton1Click:Connect(function()
    isAutoAttacking = not isAutoAttacking
    
    if isAutoAttacking then
        AutoAttackButton.BackgroundColor3 = Color3.fromRGB(70, 200, 70)
        AutoAttackButton.Text = "üó°Ô∏è Auto Attack: ON"
        AttackIndicator.BackgroundColor3 = Color3.fromRGB(70, 200, 70)
        AttackIndicator.Text = "‚öîÔ∏è Auto ON"
        AttackIndicator.Visible = true
        
        -- Start auto attack loop
        if autoAttackConnection then
            autoAttackConnection:Disconnect()
        end
        
        autoAttackConnection = game:GetService("RunService").Heartbeat:Connect(function()
            performAttack()
        end)
    else
        AutoAttackButton.BackgroundColor3 = Color3.fromRGB(200, 70, 70)
        AutoAttackButton.Text = "üó°Ô∏è Auto Attack: OFF"
        AttackIndicator.Visible = false
        
        -- Stop auto attack
        if autoAttackConnection then
            autoAttackConnection:Disconnect()
            autoAttackConnection = nil
        end
    end
end)

-- Adjust canvas size to fit all content
HackScroll.CanvasSize = UDim2.new(0, 0, 0, 425)

Perbaiki script ini karna pada saat tombol dipencet tidak begitu bagus serta tambahkan fungsi executor detector lain seperti Xeno, Solara, Delta, dll serta resize ukurannya menjadi agar lebih kecil 1,5x lebih kecil dan tombol Minimize tidak bekerja malah menghancurkan GUI, serta saat button dipencet muncul floating gui yang tidak jelas arahnya menganggu fungsi GUI lainnya dan saat kadang ada kasus toggle tidak bisa dinyalakann dan juga Dropdown gui tidak berfungsi dengan baik karna terhalang gui lain saat dipencet ( minimize jangan keybind cukup perkecil gui naik ke atas ( hanya tittle tersisa ) sepeerti minimize biasa )
