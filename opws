-- Add a subtle glow effect
local TeleportGlow = Instance.new("UIGradient")
TeleportGlow.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(70, 150, 220)),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(80, 170, 240)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(70, 150, 220))
})
TeleportGlow.Parent = TeleportButton

-- Create a glow animation for the button
spawn(function()
    local offset = 0
    while wait(0.05) do
        offset = (offset + 0.01) % 1
        TeleportGlow.Offset = Vector2.new(offset, 0)
    end
end)

