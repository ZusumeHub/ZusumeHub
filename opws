-- Teleport function
TeleportButton.MouseButton1Click:Connect(function()
    if selectedOption then
        if selectedOption == "Teleport to next quest" then
            -- Find StoryPromptREMOTE in workspace
            local storyPrompt = workspace:FindFirstChild("StoryPromptREMOTE")
            if storyPrompt and storyPrompt:IsA("BasePart") then
                LocalPlayer.Character.HumanoidRootPart.CFrame = storyPrompt.CFrame
            end
        elseif selectedOption == "Teleport to training skill" then
            -- Teleport to hardcoded training area
            LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2013.29, -193.87, 80.82)
        elseif selectedOption == "Teleport to Drop Supplies" then
            -- Find Drop folder and get any BasePart inside
            local dropFolder = workspace:FindFirstChild("Drop")
            if dropFolder and dropFolder:IsA("Folder") then
                for _, child in pairs(dropFolder:GetChildren()) do
                    if child:IsA("BasePart") then
                        local targetPos = child.Position + Vector3.new(0, 5, 0)
                        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos)
                        break
                    end
                end
            end
        end
    end
end)

-- Auto Attack System
local isAutoAttacking = false
local autoAttackConnection = nil

local function findClosestMob()
    local closestMob = nil
    local closestDistance = math.huge
    local character = LocalPlayer.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return nil end
    
    local hrp = character.HumanoidRootPart
    
    local unitFolder = workspace:FindFirstChild("Units")
    if unitFolder then
        for _, unit in pairs(unitFolder:GetChildren()) do
            -- Check if the unit is a valid target
            if unit:IsA("BasePart") then
                -- Basic validation - can be extended based on game mechanics
                local distance = (unit.Position - hrp.Position).Magnitude
                if distance < closestDistance and distance < 50 then
                    closestDistance = distance
                    closestMob = unit
                end
            end
        end
    end
    
    return closestMob
end

local function performAttack()
    local character = LocalPlayer.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    
    local target = findClosestMob()
    if target then
        -- Create attack args
        local args = {
            [1] = {
                ["Type"] = "Hit",
                ["From"] = character.HumanoidRootPart.Position,
                ["Slot"] = "Light", -- Can be changed based on attack type
                ["UID"] = target.Name -- Assuming target name is the UID
            }
        }
        
        -- Find and fire the remote event for attacks
        local remoteEvent = game:GetService("ReplicatedStorage"):FindFirstChild("Remote")
        if remoteEvent and remoteEvent:FindFirstChild("CharacterRE") then
            remoteEvent.CharacterRE:FireServer(unpack(args))
        end
    end
end

