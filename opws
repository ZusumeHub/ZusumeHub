-- ApplyStats Button Glow effect
local ApplyStatsGlow = Instance.new("UIGradient")
ApplyStatsGlow.Color = ColorSequence.new({
    ColorSequenceKeypoint.new(0, Color3.fromRGB(70, 120, 220)),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(90, 140, 240)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(70, 120, 220))
})
ApplyStatsGlow.Parent = ApplyStatsButton

-- Create a glow animation for the ApplyStats button
spawn(function()
    local offset = 0
    while wait(0.05) do
        offset = (offset + 0.01) % 1
        ApplyStatsGlow.Offset = Vector2.new(offset, 0)
    end
end)

-- Auto Collect Status
local isAutoCollectEnabled = false

-- Toggle AutoCollect function
AutoCollectToggle.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isAutoCollectEnabled = not isAutoCollectEnabled
        
        if isAutoCollectEnabled then
            -- Toggle ON
            AutoCollectToggle.BackgroundColor3 = Color3.fromRGB(60, 200, 120)
            ToggleCircle:TweenPosition(UDim2.new(0, 22, 0, 2), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, 0.3, true)
            
            -- Start auto-collecting items
            spawn(function()
                while isAutoCollectEnabled do
                    -- Find and collect items in workspace.Drop
                    local dropFolder = workspace:FindFirstChild("Drop")
                    if dropFolder then
                        for _, item in pairs(dropFolder:GetChildren()) do
                            if item:IsA("BasePart") then
                                local args = {
                                    [1] = item
                                }
                                local remoteEvent = game:GetService("ReplicatedStorage"):FindFirstChild("Remote")
                                if remoteEvent and remoteEvent:FindFirstChild("AttemptInteractionRE") then
                                    remoteEvent.AttemptInteractionRE:FireServer(unpack(args))
                                end
                            end
                        end
                    end
                    wait(0.5) -- Collect every half second
                end
            end)
        else
            -- Toggle OFF
            AutoCollectToggle.BackgroundColor3 = Color3.fromRGB(80, 80, 100)
            ToggleCircle:TweenPosition(UDim2.new(0, 2, 0, 2), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, 0.3, true)
        end
    end
end)

-- Apply stats function
ApplyStatsButton.MouseButton1Click:Connect(function()
    local speedValue = tonumber(SpeedTextBox.Text) or 1
    local jumpValue = tonumber(JumpTextBox.Text) or 50
    
    -- Limit values to reasonable ranges
    speedValue = math.min(math.max(speedValue, 0.1), 5)
    jumpValue = math.min(math.max(jumpValue, 10), 200)
    
    -- Update the character's stats
    local character = LocalPlayer.Character
    if character and character:FindFirstChild("Humanoid") then
        character.Humanoid.WalkSpeed = speedValue * 16 -- Base speed is 16
        character.Humanoid.JumpPower = jumpValue
        
        -- Flash button as visual feedback
        local originalColor = ApplyStatsButton.BackgroundColor3
        ApplyStatsButton.BackgroundColor3 = Color3.fromRGB(100, 200, 100)
        wait(0.2)
        ApplyStatsButton.BackgroundColor3 = originalColor
    end
end)

-- Tab switching functionality
HomeButton.MouseButton1Click:Connect(function()
    HomeContent.Visible = true
    HackContent.Visible = false
    
    -- Update button styling
    HomeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    HomeButtonSelect.BackgroundTransparency = 0
    
    HackButton.TextColor3 = Color3.fromRGB(190, 190, 190)
    HackButtonSelect.BackgroundTransparency = 1
end)

HackButton.MouseButton1Click:Connect(function()
    HomeContent.Visible = false
    HackContent.Visible = true
    
    -- Update button styling
    HackButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    HackButtonSelect.BackgroundTransparency = 0
    
    HomeButton.TextColor3 = Color3.fromRGB(190, 190, 190)
    HomeButtonSelect.BackgroundTransparency = 1
end)

-- Clock Updater
spawn(function()
    while true do
        local date = os.date("*t")
        local hour = date.hour
        local minute = date.min
        local second = date.sec
        local ampm = "AM"
        
        if hour >= 12 then
            ampm = "PM"
            if hour > 12 then
                hour = hour - 12
            end
        elseif hour == 0 then
            hour = 12
        end
        
        TimeDisplay.Text = string.format("%02d:%02d:%02d %s", hour, minute, second, ampm)
        DateDisplay.Text = os.date("%B %d, %Y")
        
        wait(1)
    end
end)

-- Update Player Info
spawn(function()
    UsernameLabel.Text = LocalPlayer.Name
    UserIdLabel.Text = "ID: " .. LocalPlayer.UserId
    
    -- Set up the player's avatar image
    spawn(function()
        local userId = LocalPlayer.UserId
        local thumbType = Enum.ThumbnailType.HeadShot
        local thumbSize = Enum.ThumbnailSize.Size420x420
        local content = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
        
        AvatarImage.Image = content
    end)
    
    -- Get current game info
    local success, result = pcall(function()
        return game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
    end)
    
    if success then
        GameName.Text = result.Name
    else
        GameName.Text = "Unknown Game"
    end
end)

-- Drag functionality
local dragging
local dragInput
local dragStart
local startPos

local function updateDrag(input)
    local delta = input.Position - dragStart
    MainContainer.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

TopBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = MainContainer.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

TopBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        updateDrag(input)
    end
end)

-- Hide/show functionality
local function hideGUI()
    MainContainer:TweenPosition(UDim2.new(0.5, -200, 1.5, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, 0.5, true)
end

local function showGUI()
    MainContainer:TweenPosition(UDim2.new(0.5, -200, 0.5, -175), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, 0.5, true)
end

CloseButton.MouseButton1Click:Connect(function()
    hideGUI()
end)

MinimizeButton.MouseButton1Click:Connect(function()
    if MainContainer.Position.Y.Scale < 1 then
        hideGUI()
    else
        showGUI()
    end
end)

-- Set up keybind toggle (default M key)
UIS.InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.M then
        if MainContainer.Position.Y.Scale < 1 then
            hideGUI()
        else
            showGUI()
        end
    end
end)

